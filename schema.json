{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "id": {
      "$id": "/properties/id",
      "type": "integer",
      "title": "The Id Schema ",
      "default": 0,
      "examples": [
        17720
      ]
    },
    "lat": {
      "$id": "/properties/lat",
      "type": "number",
      "title": "The Lat Schema ",
      "default": 0,
      "examples": [
        37.31492614746094
      ]
    },
    "lon": {
      "$id": "/properties/lon",
      "type": "number",
      "title": "The Lon Schema ",
      "default": 0,
      "examples": [
        13.571385383605957
      ]
    },
    "tags": {
      "$id": "/properties/tags",
      "type": "object",
      "properties": {
        "operator": {
          "$id": "/properties/tags/properties/operator",
          "type": "string",
          "title": "The Operator Schema ",
          "default": "",
          "examples": [
            "A.Nuara E Figli Srl"
          ]
        },
        "brand": {
          "$id": "/properties/tags/properties/brand",
          "type": "string",
          "title": "The Brand Schema ",
          "default": "",
          "examples": [
            "Pompe Bianche"
          ]
        },
        "source:date": {
          "$id": "/properties/tags/properties/source:date",
          "type": "string",
          "title": "The Source:date Schema ",
          "default": "",
          "examples": [
            "2018-04-11"
          ]
        }
      }
    }
  }
}
